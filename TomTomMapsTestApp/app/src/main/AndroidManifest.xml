<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tomtommapstestapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data android:name="OnlineMaps.Key" android:value="YOUR_APP_KEY_HERE" />
        <meta-data android:name="OnlineSearch.Key" android:value="YOUR_APP_KEY_HERE" />
        <!--
        <meta-data android:name="OnlineTraffic.Key" android:value="YOUR_APP_KEY_HERE" />
        <meta-data android:name="OnlineRouting.Key" android:value="YOUR_APP_KEY_HERE" />
        <meta-data android:name="GeofencingApi.Key" android:value="YOUR_APP_KEY_HERE" />
        -->
        <activity android:name=".MainActivity"
            android:resizeableActivity="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

</manifest>